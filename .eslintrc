{
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "single"],
    "strict": [2, "never"],
    "linebreak-style": [2, "unix"],
    "no-unused-vars": [2, { "argsIgnorePattern": "^_" }],
    "no-console": 1
  }
}
